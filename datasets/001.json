{
"1": {
"displayText": "بِسْمِ اللَّهِ الرَّحْمَنِ الرَّحِيمِ\r",
"text": "بسم الله الرحمن الرحيم"
},
"2": {
"displayText": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ\r",
"text": "الحمد لله رب العالمين"
},
"3": {
"displayText": "الرَّحْمَنِ الرَّحِيمِ\r",
"text": "الرحمن الرحيم"
},
"4": {
"displayText": "مَالِكِ يَوْمِ الدِّينِ\r",
"text": "مالك يوم الدين"
},
"5": {
"displayText": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ\r",
"text": "إياك نعبد وإياك نستعين"
},
"6": {
"displayText": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ\r",
"text": "اهدنا الصراط المستقيم"
},
"7": {
"displayText": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَا الضَّالِّينَ\r",
"text": "صراط الذين أنعمت عليهم غير المغضوب عليهم ولا الضالين"
}
}